{
  "name": "dart-flutter-union-with-equatable",
  "displayName": "dart-flutter-union-with-equatable",
  "description": "generate union for dart/flutter with equatable",
  "repository": {
		"type": "git",
		"url": "https://github.com/Spriggan1992/dart-union-with-equatable.git"
	},
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.80.2"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:dart"
  ],
  "keywords": [
    "vscode",
    "flutter",
    "dart",
    "union",
    "extension",
    "generator",
    "sealed",
    "classes",
    "class"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dart-flutter-union-with-equatable.generateUnion",
        "title": "Generate Union"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.80.2",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "node-sass": "^9.0.0"
  },
  "icon": "assets/icon.png",
  "author": "Roman Kozlov",
  "publisher": "RomanDart"
}
